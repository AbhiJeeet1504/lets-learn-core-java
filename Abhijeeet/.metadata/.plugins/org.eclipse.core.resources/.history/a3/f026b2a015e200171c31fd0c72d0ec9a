package Abhijeeet_Patterns;

public class Pattern_K {

	public static void main(String[] args) {

		int num=7;
		for(int row=1;row<=num;row++)
		{
			for(int col=1;col<=num;col++)
			{
				for(col=num;col>=1;col--)
				{
					
					{
					System.out.print("*");
					}
				}
				/*else
				{
					System.out.print(" ");
				}*/
			}System.out.println("");
		}
		
	}

}
