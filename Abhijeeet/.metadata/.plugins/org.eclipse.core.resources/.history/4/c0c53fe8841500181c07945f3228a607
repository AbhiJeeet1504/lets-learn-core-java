package Class170218;

import java.util.HashMap;

public class secreteSanta {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		HashMap<String, employee> emp= new HashMap<>();
		
		emp.put("ABhi", new employee(100,25,"Abhi"));
		emp.put("Avi", new employee(101,25,"Avi"));
		emp.put("Anu", new employee(102,25,"Anu"));
		emp.put("Ash", new employee(103,25,"Ash"));
		emp.put("Moni", new employee(104,25,"Moni"));
		emp.put("Swty", new employee(105,25,"Swty"));
		emp.put("Chiw", new employee(106,25,"chiw"));
		emp.put("Btoo", new employee(107,25,"B2"));
		
		
	}

}

class employee{
	int empID, age;
	String name;
	
	employee(int empID, int age, String name)
	{
		this.empID=empID;
		this.age=age;
		this.name=name;
	}
}