package Abhijeeet;

public class PatternC {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num=10;
		for(int row=1; row<=num; row++)
		{
			for(int col=1; col<=num; col++){
				if(row==1 && col==2 ||  row==num || col==1  ){
					
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
					
				}
			}
			System.out.println(" ");
		}
	}

}
