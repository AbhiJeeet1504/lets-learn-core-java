package babasaheb;

public class JPattern {

	public static void main(String[] args) {

		int num = 10;
		for (int row = 4; row <= num; row++) {
			for (int col = 2; col <= num/2; col++) {
				if (row == num || col == num / 2|| row==num/1) {

					System.out.print(" * ");
				} else {
					System.out.print("   ");
				}

			}
			System.out.println();
		}

	}

}
