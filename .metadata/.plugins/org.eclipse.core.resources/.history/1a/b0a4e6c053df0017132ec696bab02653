package babasaheb;

public class JPattern {

	public static void main(String[] args) {

		int num = 10;
		for (int row = 1; row <= num; row++) {
			for (int col = 1; col <= num/2; col++) {
				if ( row==num ||row==1||col==num/2 ||row==1) {

					System.out.print(" * ");
				} else {
					System.out.print("   ");
				}

			}
			System.out.println();
		}

	}

}
